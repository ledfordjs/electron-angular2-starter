    "karma-sourcemap-loader": "^0.3.7",
    "karma-typescript": "^2.1.5",
    "karma-webpack": "^1.8.1",
    "node-sass": "^4.1.1",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.14",
    "raw-loader": "^0.5.1",
