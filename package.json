{
  "name": "electron-angular2-starter",
  "version": "1.0.0",
  "description": "electron app starter with angular-2",
  "main": "index.js",
  "scripts": {
    "start": "webpack && npm run electron",
    "test": "$(npm bin)/karma start",
    "electron": "$(npm bin)/electron electron.js",
    "build:prod": "npm run clean:dist && $(npm bin)/webpack && $(npm bin)/electron-packager . electron_dist/electronStarter",
    "build:dev": "$(npm bin)/webpack",
    "clean:dist": "rimraf -- dist",
    "clean:app": "rimraf -- "
